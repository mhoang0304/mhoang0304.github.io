extends ./template-part/layout.pug

block title
  title Frequency - Trang Chủ
  link(rel="stylesheet" href="./css/index.css")

block banner
  .banner
    img(src="./img/banner-1.jpg" alt="banner")
    .banner__overlay
    .banner__content
      h1.banner__content-h1 SỰ TỰ TIN
      h1.banner__content-h1 NẰM TRÊN CỔ TAY CỦA BẠN
      h2.banner__content-h2 HÃY TẠO NÊN TRUYỀN THỐNG CỦA RIÊNG MÌNH

block content
  .main
    .container.poster
      .row.d-flex.flex-column-reverse.flex-md-row
        .col-12.col-md-6.col-lg-5.col-xl-4.d-sm-flex.flex-md-column.gx-sm-0
          .poster__hashtag
            p.poster__hashtag-title Ai nói bạn không thể lựa chọn gia đình?
            p.poster__hashtag-title Hãy để chúng tôi là câu trả lời
            a(href="#").poster__hashtag-title-link #FREQUENCY
          .d-flex.justify-content-center.align-items-sm-center.poster__collection
