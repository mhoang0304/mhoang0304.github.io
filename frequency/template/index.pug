            h1.description__main-h1 TÌM KIẾM MỘT CHIẾC ĐỒNG HỒ THÔNG MINH
            p.description__main-content Những chiếc đồng hồ được thiết kế từ những người thợ giỏi. Chúng phù hợp với cá tính, phong cách riêng của chính bạn
            button.description__main-btn
              a(href="#") XEM ĐỒNG HỒ
        .col-12.col-md-6.gx-sm-0
          img(src="./img/banner-watch-women.jpg" alt="banner")

      .row
        - var items = [{img: "./img/watch-w1.jpg", style: "HAMILTON - 32MM", name: "ANNE", price: "9.299.000 ₫"},{img: "./img/watch-w2.jpg", style: "HAMILTON - 32MM", name: "CORA", price: "12.209.000 ₫"},{img: "./img/watch-w3.jpg", style: "MORAINE - 28MM", name: "LINGER", price: "8.099.000 ₫"},{img: "./img/watch-w4.jpg", style: "HAMILTON - 32MM", name: "SYLVIA", price: "9.199.000 ₫"}]

        each card in items
          .col-6.col-md-3.d-flex.justify-content-center
            .watch
              a(href="#").watch__img 
                img(src= card.img alt= card.name)
              a(href="#").watch__info
                p.watch__info-style= card.style
                p.watch__info-name= card.name
                p.watch__info-price= card.price

