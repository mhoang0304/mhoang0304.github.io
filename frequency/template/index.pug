        .col-12.col-md-6.col-lg-7.col-xl-8.gx-sm-0
          img(src="./img/banner-2.jpg" alt="banner")

    .container
      .row
        .col-12.col-md-6.gx-sm-0
          img(src="./img/banner-watch-men.jpg" alt="banner")
        .col-12.col-md-6.gx-sm-0.description
          .description__main
            p.description__main-title Đồng Hồ Nam
            h1.description__main-h1 TÌM KIẾM MỘT CHIẾC ĐỒNG HỒ THÔNG MINH
            p.description__main-content Những chiếc đồng hồ được thiết kế từ những người thợ giỏi. Chúng phù hợp với cá tính, phong cách riêng của chính bạn
            button.description__main-btn 
              a(href="#") XEM ĐỒNG HỒ

      .row
        - var items = [{img: "./img/watch-m1.jpg", style: "WEIMAR - 40MM", name: "STEFAN", price: "8.099.000 ₫"},{img: "./img/watch-m2.jpg", style: "COLOSSEUM - 42MM", name: "MACE", price: "8.099.000 ₫"},{img: "./img/watch-m3.jpg", style: "COLOSSEUM - 42MM", name: "GUANDAO", price: "9.299.000 ₫"},{img: "./img/watch-m4.jpg", style: "COLOSSEUM - 42MM", name: "SLING", price: "7.099.000 ₫"}]

        each card in items
          .col-6.col-md-3.d-flex.justify-content-center
            .watch
              a(href="#").watch__img
                img(src= card.img alt= card.name)
              a(href="#").watch__info
                p.watch__info-style= card.style
                p.watch__info-name= card.name
                p.watch__info-price= card.price

      .row.d-flex.flex-column-reverse.flex-md-row
        .col-12.col-md-6.gx-sm-0.description
